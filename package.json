{
  "name": "@knowcode/doc-builder",
  "version": "1.10.8",
  "description": "Reusable documentation builder for markdown-based sites with Vercel deployment support",
  "main": "index.js",
  "bin": {
    "doc-builder": "scripts/npx-runner.js"
  },
  "scripts": {
    "postinstall": "node scripts/setup.js || true",
    "test": "node cli.js build && echo \"Build test passed\"",
    "build": "node cli.js build"
  },
  "keywords": [
    "documentation",
    "markdown",
    "static-site-generator",
    "vercel",
    "notion-style",
    "doc-builder"
  ],
  "author": "KnowCode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wapdat/doc-builder.git"
  },
  "bugs": {
    "url": "https://github.com/wapdat/doc-builder/issues"
  },
  "homepage": "https://github.com/wapdat/doc-builder#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.12",
    "ora": "5.4.1",
    "prompts": "^2.4.2",
    "semver": "^7.7.2"
  },
  "files": [
    "cli.js",
    "index.js",
    "lib/**/*",
    "assets/**/*",
    "scripts/**/*",
    "templates/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
